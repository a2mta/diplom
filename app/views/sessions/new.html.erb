<h1>Sign in</h1>
<%= form_for(:session, :url => sessions_path) do |f| %>
    <div class="filed">
      <%= f.label :email %>
      <%= f.text_field :email %>
      <div class="filed">
        <%= f.label :password %>
        <%= f.password_field :password %>
      </div>
      <div class="filed">
        <%= f.submit "Sign in" %>
      </div>
    </div>
<% end %>

<p>New user? <%= link_to "Sign ip now!", signup_path %></p>